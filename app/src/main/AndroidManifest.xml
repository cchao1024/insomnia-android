<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.cchao.sleeping">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".global.App"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <service
            android:name="com.cchao.simplemusic.MusicService"
            android:enabled="true"
            android:exported="true">
        </service>

        <activity android:name=".ui.fall.FallImageActivity"/>
        <activity android:name=".ui.global.ImageShowActivity"/>
        <activity android:name=".ui.music.MusicPlayerActivity"/>
        <activity android:name=".ui.music.MusicListActivity"/>
        <activity android:name=".ui.fall.FallMusicActivity"/>
    </application>

</manifest>